<h1> MOCKING PRODUCTS </h1>

{{#each products}}
  <h2>{{this.title}}</h2>
  <p>Id: {{this.id}}</p>
  <p>Description: {{this.description}}</p>
  <p>Price: ${{this.price}}</p>
  <p>Img: {{this.thumbnails}}:</p>
  <img src="{{this.thumbnails}}" alt="alt" width="200" height="200" />
  <p>Code: {{this.code}}</p>
  <p>Stock: {{this.stock}}</p>
  <p>Status: {{this.status}}</p>
  <p>Category: {{this.category}}</p>
{{/each}}